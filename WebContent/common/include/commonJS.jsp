// Str 값이 len 초과시 ... 으로 출력
function stringCut(str, len){
	var l = 0;
	var cutStr;
	if(str.length > len){
		for(var i=0;i<str.length;i++){
			l +=(str.charCodeAt(i) > 128) ? 2 : 1;
			if(l > len) cutStr = str.substring(0,len) + "...";
		}
	}else{
		cutStr = str;
	}
	document.write(cutStr);
}
</script>
